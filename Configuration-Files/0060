// ç”Ÿæˆä¸€ä¸ªå°äº max çš„éšæœºæ•´æ•°
function getRandomInt(max) {
  return Math.floor(Math.random() * max);
}

// æ£€æŸ¥å¹¶è¿”å›æœ‰æ•ˆçš„å›½å®¶ä¿¡æ¯
function country_ValidCheck(para) {
  return para || defaultcountry;
}

// æ£€æŸ¥å¹¶è¿”å›æœ‰æ•ˆçš„åœ°åŒºä¿¡æ¯
function Region_ValidCheck(para) {
  if (para) {
    if (para.toLowerCase() === 'kowloon') {
      return 'ä¹é¾™åŸåŒº';
    if (para.toLowerCase() === 'åƒè‘‰ç¸£') {
      return 'åƒå¶å¿';
    if (para.toLowerCase() === 'Mazovia') {
      return 'æ‘©æ‹‰ç»´äºš';
    if (para.toLowerCase() === 'North Chungchong') {
      return 'å¿ æ¸…åŒ—é“';
    if (para.toLowerCase() === 'Stockholm-County') {
      return 'æ–¯å¾·å“¥å°”æ‘©';
    if (para.toLowerCase() === 'Ang Thong') {
      return 'çº¢ç»Ÿåºœ';
    if (para.toLowerCase() === 'å¥§å¼—æ¶…-ç¾…è¨¥-é˜¿çˆ¾å‘æ–¯å¤§å€') {
      return 'é˜¿å°”å‘æ–¯å¤§åŒº';
    if (para.toLowerCase() === 'Hawalli') {
      return 'å“ˆç“¦åˆ©çœ';
    if (para.toLowerCase() === 'Taoyuan') {
      return 'åƒå¶å¿æ¡ƒå›­å¸‚';
    if (para.toLowerCase() === 'åŠ å·') {
      return 'åŠ åˆ©ç¦å°¼äºšå·';
    }
    return para;
  } else {
    return region0;
    // return emojis[getRandomInt(emojis.length)]; // å¯é€‰ï¼šè¿”å›éšæœºè¡¨æƒ…
  }
}

// æ£€æŸ¥å¹¶è¿”å›æœ‰æ•ˆçš„åŸå¸‚ä¿¡æ¯
function City_ValidCheck(para) {
  return para || defaultCity;
}



// æ£€æŸ¥å¹¶è¿”å›æœ‰æ•ˆçš„ ISP ä¿¡æ¯
function ISP_ValidCheck(para) {
  return para || defaultISP;
}

// æ£€æŸ¥å¹¶è½¬æ¢åœ°åŒºåç§°
function Area_check(para) {
  return para === "ä¸­åæ°‘å›½" ? "å°æ¹¾" : para;
}

