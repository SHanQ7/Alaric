// è„šæœ¬å¼€å§‹
let body = $response.body;
let obj = JSON.parse(body);

// æå–åœ°åŒºä¿¡æ¯
var region = obj['regionName'] || defaultRegion;

// æ ¼å¼åŒ–æ ‡é¢˜ï¼ˆç¬¬1è¡Œï¼‰ï¼šå›½æ—— + å›½å®¶å
var title = flags.get(obj['countryCode']) + ' ' + obj['country'] + ' ' + Region_ValidCheck(para)(obj['regionName']);

// æ ¼å¼åŒ–å‰¯æ ‡é¢˜ï¼ˆç¬¬2è¡Œï¼‰ï¼šåŸå¸‚å + ISP å
var subtitle = City_ValidCheck(obj['city']) + ' ' + obj['query'] + ' ' + ISP_ValidCheck(obj['org']);

// IP ä¿¡æ¯
var ip = obj['query'];

// é•¿æŒ‰èŠ‚ç‚¹é€‰æ‹©â€œæŸ¥çœ‹èŠ‚ç‚¹ä¿¡æ¯â€æ—¶çš„ä¿¡æ¯
var description = `æœåŠ¡å•†: ${obj['isp']}\nåœ°åŒº: ${Region_ValidCheck(region)}\nåŸå¸‚: ${City_ValidCheck(obj['city'])}\nIP: ${obj['query']}\næ—¶åŒº: ${obj['timezone']}`;

// å®Œæˆå¹¶è¿”å›å¤„ç†ç»“æœ
$done({ title, subtitle, ip, description });
