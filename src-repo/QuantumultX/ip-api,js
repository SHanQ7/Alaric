if ($response.statusCode != 200) {
  $done(null);
}

function t2s(str) {
  if (!str) return "";
  const map = {"è–©":"è¨","æ¿Ÿ":"æµ","æ©‹":"æ¡¥","é­¯":"é²","æº«":"æ¸©", "å€":"åŒº", "å¥§":"å¥¥", "ç¾…":"ç½—", "çˆ¾":"å°”", "ç¸£":"å¿", "è˜­":"å…°", "è¯":"å","å£¢":"åœ","è£":"é‡Œ", "ç´„":"çº¦", "ç†±":"çƒ­", "å…§":"å†…", "ç›§":"å¢","è¬":"ä¸‡","èˆ‡":"ä¸","å°ˆ":"ä¸“","æ¥­":"ä¸š","æ±":"ä¸œ","çµ²":"ä¸","ä¸Ÿ":"ä¸¢","å…©":"ä¸¤","åš´":"ä¸¥","å–ª":"ä¸§","å€‹":"ä¸ª","è±":"ä¸°","è‡¨":"ä¸´","ç‚º":"ä¸º","éº—":"ä¸½","èˆ‰":"ä¸¾","éº¼":"ä¹ˆ","ç¾©":"ä¹‰","çƒ":"ä¹Œ","æ¨‚":"ä¹","å–¬":"ä¹”","ç¿’":"ä¹ ","é„‰":"ä¹¡","æ›¸":"ä¹¦","è²·":"ä¹°","äº‚":"ä¹±","çˆ­":"äº‰","æ–¼":"äº","è™§":"äº","é›²":"äº‘","äº":"äºš","ç”¢":"äº§","ç•":"äº©","è¦ª":"äº²","å„„":"äº¿","åƒ¹":"ä»·","å„€":"ä»ª","å„„":"äº¿","å„ª":"ä¼˜","æœƒ":"ä¼š","å‚´":"ä¼›","å‰":"ä¼Ÿ","å‚³":"ä¼ ","å‚·":"ä¼¤","å€«":"ä¼¦","å½":"ä¼ª","é«”":"ä½“","ä½‡":"ä¼«","ä½ˆ":"å¸ƒ","ä½”":"å ","å”":"å","å–®":"å•","è¡›":"å«","å»":"å´","å» ":"å‚","æ­·":"å†","åƒ":"å‚","é›™":"åŒ","ç™¼":"å‘","è®Š":"å˜","å¢":"ä¸›","å°":"å°","è‘‰":"å¶","å¾Œ":"å","å“¡":"å‘˜","å•":"é—®","åœ‹":"å›½","åœ–":"å›¾","åœ":"å›´","åœ’":"å›­","åœ˜":"å›¢","å£":"å","å£©":"å","å£¯":"å£®","è²":"å£°","å£º":"å£¶","è™•":"å¤„","å‚™":"å¤‡","å¾©":"å¤","å¤ ":"å¤Ÿ","å¤¢":"æ¢¦","å¥ª":"å¤º","å¥®":"å¥‹","å©¦":"å¦‡","å”":"å","çµ•":"ç»","æ„›":"çˆ±","è§€":"è§‚","è¨´":"è¯‰","å°‹":"å¯»","å°":"å¯¼","å°":"å¯¹","å±¬":"å±","å²¡":"å†ˆ","å³¶":"å²›","å´—":"å²—","å³½":"å³¡","å¶º":"å²­","å¶½":"å²³","å·–":"å²©","å¹£":"å¸","å»£":"å¹¿","å»³":"å…","æ‡‰":"åº”","å»Ÿ":"åº™","åº«":"åº“","å»¢":"åºŸ","é–‹":"å¼€","å¼µ":"å¼ ","å½ˆ":"å¼¹","å¼·":"å¼º","æ­¸":"å½’","ç•¶":"å½“","éŒ„":"å½•","å½™":"æ±‡","å½ˆ":"å¼¹","æ‡·":"æ€€","ç¸½":"æ€»","æˆ°":"æˆ˜","æˆ¶":"æˆ·","æ’¥":"æ‹¨","æ“´":"æ‰©","æ“‡":"æ‹©","æ“Š":"å‡»","æ“ ":"æŒ¤","æ“":"æ‹¥","æ“¬":"æ‹Ÿ","æ“¾":"æ‰°","æ•µ":"æ•Œ","æ•¸":"æ•°","æ–·":"æ–­","æ™‚":"æ—¶","æ™‰":"æ™‹","æ›†":"å†","æœƒ":"ä¼š","æœ§":"èƒ§","æ±":"ä¸œ","æ¥µ":"æ","æ¨™":"æ ‡","æ¦®":"è£","æ§‹":"æ„","æ¨£":"æ ·","æ¨¸":"æœ´","æ¨¹":"æ ‘","æ©Ÿ":"æœº","æ©«":"æ¨ª","æª”":"æ¡£","æª¢":"æ£€","æ­¡":"æ¬¢","æ­²":"å²","æ­·":"å†","æ­":"æ¬§","æ®˜":"