// å›½å®¶æ˜ å°„è¡¨
function country_ValidCheck(para) {
   const countryMap = {
     "ä¸­è¯æ°‘åœ‹" : "å°æ¹¾",
     "ä¸­åæ°‘å›½" : "å°æ¹¾",
     "ä¿„ç½—æ–¯è”é‚¦" : "ä¿„ç½—æ–¯",
     "å¾·æ„å¿—è”é‚¦å…±å’Œå›½" : "å¾·å›½",
     "Libya" : "åˆ©æ¯”äºš",
 };
 para = para ? para.trim() : "";
 return countryMap[para] || para || country0;
};

// åœ°åŒºæ˜ å°„è¡¨
function Area_check(para) {
  const areaMap = {
    // AE - é˜¿æ‹‰ä¼¯è”åˆé…‹é•¿å›½ - United Arab Emirates
    "Dubai" : "è¿ªæ‹œ",
    "Fujairah" : "å¯ŒæŸ¥ä¼Šæ‹‰",
    "ImÄrat Umm al Qaywayn" : "ä¹Œå§†ç›–ä¸‡",

